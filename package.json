    "Shoperti",
    "template",
    "theme"
  ],
  "author": "Shoperti",
  "license": "ISC",
  "engineStrict": true,
  "engines": {
    "node": ">= 6.9.1",
    "npm": ">= 4.0.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
