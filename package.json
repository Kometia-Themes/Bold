{
  "name": "themebuilder",
  "version": "1.0.0",
  "description": "Theme Builder Starter",
  "repository": {
    "type": "git",
    "url": "https://heliumdev@bitbucket.org/heliumdev/webpack2.git"
  },
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack -d --watch --progress --colors --display-error-details",
